{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "deploy": "react-router build && NODE_ENV=production wrangler deploy",
    "deploy:version": "react-router build && wrangler versions upload",
    "deploy:promote": "wrangler versions deploy",
    "start": "wrangler dev --test-scheduled",
    "typecheck": "tsc",
    "typegen": "wrangler types && react-router typegen",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "db:seed": "wrangler d1 execute DB --local --file=./seed-01.sql",
    "db:seed-prod": "wrangler d1 execute DB --remote --file=./seed-01.sql",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:apply": "wrangler d1 migrations apply DB --local",
    "db:apply-prod": "wrangler d1 migrations apply DB --remote"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@react-router/cloudflare": "^7.5.0",
    "better-auth": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "~0.44.2",
    "isbot": "^5.1.26",
    "lucide-react": "^0.479.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-router": "^7.5.0",
    "react-turnstile": "^1.1.4",
    "remix-toast": "^3.1.0",
    "session-context": "^0.0.6",
    "sonner": "^1.7.4",
    "spin-delay": "^2.0.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20250412.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@react-router/dev": "^7.5.0",
    "@tailwindcss/vite": "^4.1.3",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "drizzle-kit": "~0.30.6",
    "lefthook": "^1.11.9",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.11.1"
  }
}
